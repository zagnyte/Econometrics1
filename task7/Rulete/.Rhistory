library(prob)
probspace(roulette(1), makespace=TRUE)
n <- 100000
bal <- c(rep(0, times=n))
teig_bal <- c(rep(0, times=n))
for (i in 1:n) {
bal[i] <- sum(sample(c(1,-1), size=1000, replace=TRUE, prob=c(18/37, 19/37)))
if (bal[i] > 0) {
teig_bal[i] <- 1
}
else {
teig_bal[i] <- 0
}
}
mean(teig_bal)
x <- quantile(bal, probs=0.25)
x
